# Tipos
type User {
  id: ID
  email: String
  password: String
  roleId: ID
}

# Consultas
type Query {
  getAllUsers: [User]
  getUserById(id: ID): User
}

# Mutaciones
type Mutation {
  createUser(email: String, password: String, roleId: ID): Boolean
  updateUser(id: ID, email: String, password: String, roleId: ID): Boolean
  deleteUser(id: ID): Boolean
}
